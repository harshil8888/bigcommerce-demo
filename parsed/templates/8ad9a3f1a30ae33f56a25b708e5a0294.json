{"components/common/footer":"<footer class=\"txt-color-light-white\">\n\t<div class=\"footer-content footer-container\">\n    <div class=\"footer-top-content flex flex-justify-between flex-items-start flex-align-start\">\n      <div class=\"logo-content\">\n\t\t\t\t<a href=\"/\" class=\"footer-logo\">\n\n\t\t\t\t\t{{#if settings.store_logo.image}}\n\n\t\t\t\t\t\t<img src=\"/product_images/uploaded_images/maku-white-footer-logo.png\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n\n\t\t\t\t\t{{else}}\n\n\t\t\t\t\t\t<span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n\n\t\t\t\t\t{{/if}}\n\n\t\t\t\t</a>\n\t\t\t</div>\n\t\t\t<div class=\"footer-menu-shop footer-menu\">\n\t\t\t\t<h4> {{lang 'footer.categories'}} </h4>\n\t\t\t\t<ul>\n\t\t\t\t\t{{#each categories}}\n\t\t\t\t\t\t<li>\n\t\t\t\t\t\t\t<a href=\"{{url}}\">\n\t\t\t\t\t\t\t\t<span class=\"site-nav__label\">{{name}}</span>\n\t\t\t\t\t\t\t</a>\n\t\t\t\t\t\t</li>\n\t\t\t\t\t{{/each}}\n\t\t\t\t</ul>\n\t\t\t</div>\n\t\t\t<div class=\"footer-menu-learn footer-menu\">\n\t\t\t\t<h4> {{lang 'footer.navigate'}}  </h4>\n\t\t\t\t<ul>\n\t\t\t\t\t{{#each pages}}\n\t\t\t\t\t\t<li>\n\t\t\t\t\t\t\t<a href=\"{{url}}\">\n\t\t\t\t\t\t\t\t<span class=\"site-nav__label\">{{name}}</span>\n\t\t\t\t\t\t\t</a>\n\t\t\t\t\t\t</li>\n\t\t\t\t\t{{/each}}\n\t\t\t\t</ul>\n\t\t\t</div>\n\t\t\t<div class=\"footer-menu-learn footer-menu\">\n\t\t\t  <h4>{{lang 'footer.brands'}}</h4>\n\t\t\t\t<ul class=\"footer-info-list\">\n\t\t\t\t\t{{#each shop_by_brand}}\n\t\t\t\t\t\t\t<li>\n\t\t\t\t\t\t\t\t<a href=\"{{url}}\">\n\t\t\t\t\t\t\t\t\t<span class=\"site-nav__label\">{{name}}</span>\n\t\t\t\t\t\t\t\t</a>\n\t\t\t\t\t\t\t</li>\n\t\t\t\t\t{{/each}}\n\t\t\t\t\t<li>\n\t\t\t\t\t\t<a href=\"{{urls.brands}}\">\n\t\t\t\t\t\t\t<span class=\"site-nav__label\">{{lang 'common.view_all'}}</span>\n\t\t\t\t\t\t</a>\n\t\t\t\t\t</li>\n\t\t\t\t</ul>\n\t\t\t</div>\n\t\t\t<div class=\"footer-menu-learn footer-menu\">\n\t\t\t\t<h4>{{lang 'social.connect'}}</h4>\n\t\t\t\t{{#if social_media}}\n\t\t\t    <ul>\n\t\t        {{#each social_media}}\n\t            <li >\n                <a href=\"{{url}}\" target=\"_blank\">\n                  <span class=\"site-nav__label\">{{display_name}}</span>\n                </a>\n\t            </li>\n\t\t        {{/each}}\n\t\t\t    </ul>\n\t\t\t\t{{/if}}\n\t\t\t</div>\n    </div>\n\t\t<div class=\"footer-bottom-content flex flex-align-start flex-items-start flex-justify-between flex-nowrap\">\n\t\t\t<div class=\"footer-bottom-left\">\n\t\t\t\t{{{lang 'footer.description'}}}\n\t\t\t</div>\n\t\t\t<div class=\"footer-bottom-right flex flex-justify-end flex-direction-column flex-align-between\">\n\t\t\t\t<div class=\"payment-img\">\n\t\t\t\t{{> components/common/payment-icons}}\n\t\t\t\t</div>\n\t\t\t\t{{#if theme_settings.show_copyright_footer}}\n\t        <div class=\"copyright\">\n            <p>&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} </p>\n\t        </div>\n        {{/if}}\n\t\t\t</div>\n\t\t</div>\n  </div>\n</footer>\n\n<!--{{#if banners.bottom}}\n    {{#if settings.data_tag_enabled}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            {{#each (limit banners.bottom_metadata 1)}}\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\n                    <div data-event-type=\"promotion-click\">\n                        {{{this.content}}}\n                    </div>\n                </div>\n            {{/each}}\n        </div>\n    {{else}}\n        <div class=\"banners\" data-banner-location=\"bottom\">\n            <div class=\"banner\">\n                {{{limit banners.bottom 1}}}\n            </div>\n        </div>\n    {{/if}}\n\n{{/if}} -->\n<!--\n<footer class=\"footer\" role=\"contentinfo\">\n    <div class=\"container\">\n        {{#if theme_settings.social_icon_placement_bottom '!==' 'bottom_none'}}\n            <article class=\"footer-info-col\n                footer-info-col--social\n                footer-info-col--{{#if theme_settings.social_icon_placement_bottom '===' 'bottom_left'}}left{{else}}right{{/if}}\"\n                data-section-type=\"footer-webPages\">\n                    <h5 class=\"footer-info-heading\">{{lang 'social.connect'}}</h5>\n                    {{> components/common/social-links}}\n            </article>\n        {{/if}}\n        <section class=\"footer-info\">\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\n                <h5 class=\"footer-info-heading\">{{lang 'footer.navigate'}}</h5>\n                <ul class=\"footer-info-list\">\n                    {{#each pages}}\n                        <li>\n                            <a href=\"{{url}}\">{{name}}</a>\n                        </li>\n                    {{/each}}\n                    <li>\n                        <a href=\"{{urls.sitemap}}\">{{lang 'sitemap.page_title'}}</a>\n                    </li>\n                </ul>\n            </article>\n\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\">\n                <h5 class=\"footer-info-heading\">{{lang 'footer.categories'}}</h5>\n                <ul class=\"footer-info-list\">\n                    {{#each categories}}\n                        <li>\n                            <a href=\"{{url}}\">{{name}}</a>\n                        </li>\n                    {{/each}}\n                </ul>\n            </article>\n\n            {{#and theme_settings.shop_by_brand_show_footer shop_by_brand.length}}\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-brands\">\n                <h5 class=\"footer-info-heading\">{{lang 'footer.brands'}}</h5>\n                <ul class=\"footer-info-list\">\n                    {{#each shop_by_brand}}\n                        <li>\n                            <a href=\"{{url}}\">{{name}}</a>\n                        </li>\n                    {{/each}}\n                    <li><a href=\"{{urls.brands}}\">{{lang 'common.view_all'}}</a></li>\n                </ul>\n            </article>\n            {{/and}}\n\n            {{#if settings.address}}\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"storeInfo\">\n                <h5 class=\"footer-info-heading\">{{lang 'footer.info'}}</h5>\n                <address>{{nl2br settings.address}}</address>\n                {{#if settings.phone_number}}\n                    <strong>{{lang 'footer.call_us' phone_number=settings.phone_number}}</strong>\n                {{/if}}\n            </article>\n            {{/if}}\n\n            <!-- <article class=\"footer-info-col\" data-section-type=\"newsletterSubscription\">\n                {{#if settings.show_newsletter_box}}\n                    {{> components/common/subscription-form}}\n                {{/if}}\n                {{> components/common/payment-icons}}\n            </article>\n        </section>\n        {{#if theme_settings.geotrust_ssl_common_name}}\n            <div class=\"footer-geotrust-ssl-seal\">\n                {{> components/common/geotrust-ssl-seal}}\n            </div>\n        {{/if}}\n        {{#if theme_settings.show_powered_by}}\n            <div class=\"footer-copyright\">\n                <p class=\"powered-by\">Powered by <a href=\"https://www.bigcommerce.com?utm_source=merchant&amp;utm_medium=poweredbyBC\" rel=\"nofollow\">BigCommerce</a></p>\n            </div>\n        {{/if}}\n        {{#if theme_settings.show_copyright_footer}}\n            <div class=\"footer-copyright\">\n                <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} </p>\n            </div>\n        {{/if}}\n    </div>\n</footer> -->\n","components/common/payment-icons":"{{#with theme_settings}}\n    {{#or show_accept_amex show_accept_discover show_accept_mastercard show_accept_paypal show_accept_visa show_accept_amazonpay show_accept_googlepay}}\n    <div class=\"footer-payment-icons\">\n        {{#if show_accept_amex}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-american-express\"></use></svg>\n        {{/if}}\n        {{#if show_accept_discover}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-discover\"></use></svg>\n        {{/if}}\n        {{#if show_accept_mastercard}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-mastercard\"></use></svg>\n        {{/if}}\n        {{#if show_accept_paypal}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-paypal\"></use></svg>\n        {{/if}}\n        {{#if show_accept_visa}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-visa\"></use></svg>\n        {{/if}}\n        {{#if show_accept_amazonpay}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-amazonpay\"></use></svg>\n        {{/if}}\n        {{#if show_accept_googlepay}}\n        <svg class=\"footer-payment-icon\"><use xlink:href=\"#icon-logo-googlepay\"></use></svg>\n        {{/if}}\n    </div>\n    {{/or}}\n{{/with}}\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/subscription-form":"<h3>{{lang 'newsletter.subscribe'}}</h3>\n<p>{{lang 'newsletter.subscribe_intro'}}</p>\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n","components/common/geotrust-ssl-seal":"<table width=\"135\" border=\"0\" cellpadding=\"2\" cellspacing=\"0\" title=\"Click to Verify - This site chose GeoTrust SSL for secure e-commerce and confidential communications.\">\n    <tr>\n        <td width=\"135\" align=\"center\" valign=\"top\">\n            <script type=\"text/javascript\" src=\"https://seal.geotrust.com/getgeotrustsslseal?host_name={{theme_settings.geotrust_ssl_common_name}}&amp;size={{theme_settings.geotrust_ssl_seal_size}}&amp;lang={{locale_name}}\"></script><br />\n            <a href=\"http://www.geotrust.com/ssl/\" target=\"_blank\"  style=\"color:#000000; text-decoration:none; font:bold 7px verdana,sans-serif; letter-spacing:.5px; text-align:center; margin:0px; padding:0px;\"></a>\n        </td>\n    </tr>\n</table>\n"}