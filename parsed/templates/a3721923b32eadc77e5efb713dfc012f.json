{"components/products/options/set-rectangle":"<div class=\"form-field flex flex-items-start flex-align-start flex-justify-between flex-nowrap mobile-flex-column\" data-product-attribute=\"set-rectangle\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{this.display_name}}:\n        <span data-option-value></span>\n        <!--\n        {{#if required}}\n            <small>{{lang 'common.required'}}</small>\n        {{/if}} -->\n    </label>\n    <div class=\"cbd-option flex flex-direction-column flex-items-stretch\">\n      {{#each this.values}}\n          <input\n              class=\"form-radio\"\n              type=\"radio\"\n              id=\"attribute_rectangle__{{../id}}_{{id}}\"\n              name=\"attribute[{{../id}}]\"\n              value=\"{{id}}\"\n              {{#if selected}}\n                  checked\n                  data-default\n              {{/if}}\n              {{#if ../required}}required{{/if}}>\n          <label class=\"form-option\" for=\"attribute_rectangle__{{../id}}_{{id}}\" data-product-attribute-value=\"{{id}}\">\n              <span class=\"form-option-variant\">{{this.label}}</span>\n          </label>\n      {{/each}}\n      {{#if theme_settings.show_custom_fields_tabs '!==' true}}\n          {{#each product.custom_fields}}\n              <dt class=\"productView-info-name\">{{name}}:</dt>\n              <dd class=\"productView-info-value\">{{{value}}}</dd>\n          {{/each}}\n      {{/if}}\n    </div>\n</div>\n"}